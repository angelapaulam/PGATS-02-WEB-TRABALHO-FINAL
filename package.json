{
  "name": "trabalho-final-pgats-automacao-web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "clean:reports": "rimraf ./cypress/reports/*.json ./cypress/reports/*.html",
    "merge:reports": "npx mochawesome-merge ./cypress/reports/*.json -o ./cypress/reports/merged-report.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cypress": "^15.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "rimraf": "^6.0.1"
  },
  "reporter": "mochawesome",
  "reporterOptions": {
    "reportDir": "cypress/reports",
    "overwrite": false,
    "html": true,
    "json": true
  }
}
